# =======================
# Reddit API Credentials
# =======================
REDDIT_USER_AGENT=your_reddit_user_agent  # User-Agent string for Reddit API requests
REDDIT_CLIENT_ID=your_reddit_client_id    # OAuth client ID from Reddit
REDDIT_CLIENT_SECRET=your_reddit_client_secret  # OAuth client secret from Reddit
REDDIT_USERNAME=your_reddit_username      # Reddit account username
REDDIT_PASSWORD=your_reddit_password      # Reddit account password
REDDIT_TOKEN=your_reddit_token            # OAuth access token (if manually set)
REDDIT_SUBREDDIT=your_reddit_subreddit    # Target subreddit for posts or interactions

# =======================
# Discord Webhook
# =======================
DISCORD_WEBHOOK_URL=your_discord_webhook_url  # Webhook URL for sending messages to Discord
